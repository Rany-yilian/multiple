    <modelVersion>4.0.0</modelVersion>

    <artifactId>multiple-tool</artifactId>
    <packaging>jar</packaging>
    <name>multiple-tool</name>

    <build>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <includes>
                    <include>**/*</include>
                </includes>
